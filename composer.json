{
	"name": "leight-core/edde",
	"license": "Apache-2.0",
	"type": "library",
	"description": "The Legacy of Edde Framework in much smaller and simpler library.",
	"keywords": [
		"php",
		"framework"
	],
	"authors": [
		{
			"name": "Marek Hanzal",
			"email": "marek.hanzal@x32.cz"
		}
	],
	"config": {
		"optimize-autoloader": true,
		"bin-compat": "full",
		"discard-changes": true,
		"allow-plugins": {
			"composer/package-versions-deprecated": true
		}
	},
	"autoload": {
		"files": [
			"./src/ClanCats/Hydrahon/Query/Sql/Select.php"
		],
		"exclude-from-classmap": [
			"./src/Edde/Bridge/",
			"./vendor/clancats/hydrahon/src/Query/Sql/Select.php"
		],
		"classmap": [
			"src/",
			"tests/"
		]
	},
	"require": {
		"php": ">=8.0",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-zlib": "*",
		"ext-zip": "*",
		"psr/log": "^1.1",
		"ramsey/uuid": "^4.1.1",
		"tracy/tracy": "^v2.8",
		"symfony/http-foundation": "^v5.4.1",
		"doctrine/annotations": "^1.12",
		"robmorgan/phinx": "^0.12",
		"dibi/dibi": "^4.1",
		"phpoffice/phpspreadsheet": "^1.16",
		"nelexa/zip": "^4.0.1",
		"league/uri": "^6.4",
		"symfony/process": "^5.2",
		"diff/diff": "^3.2",
		"symfony/yaml": "^6.0.0",
		"nikic/php-parser": "*",
		"cache/void-adapter": "^1.1",
		"gabrielelana/byte-units": "^0.5.0",
		"league/flysystem": "^2.2.3",
		"clancats/hydrahon": "^1.1",
		"php-di/php-di": "^6.3",
		"nette/utils": "^3.2",
		"nyholm/psr7": "^1.3",
		"nyholm/psr7-server": "^1.0.0",
		"php-di/slim-bridge": "^3.0",
		"minwork/array": "^v1.13.5",
		"minime/annotations": "^3.1.0",
		"ocramius/proxy-manager": "^2.13.0"
	},
	"require-dev": {
		"phpunit/phpunit": ">=8.5.21"
	}
}
